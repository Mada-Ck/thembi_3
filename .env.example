# Environment variables for thembi_community_initiative

# Database connection string (PostgreSQL)
DATABASE_URL=postgres://user:password@localhost:5432/thembi

# Payload CMS secret
PAYLOAD_SECRET=your_long_random_secret

# OAuth configuration
OAUTH_SERVER_URL=https://your-oauth-provider.example.com

# Paychangu configuration
PAYCHANGU_PUBLIC_KEY=pub_live_your_key_here
PAYCHANGU_SECRET_KEY=sec_live_your_key_here

# Other optional vars
# JWT_SECRET=your_jwt_cookie_secret
# OWNER_OPEN_ID=openid-of-admin-user
# PORT=3000
# FRONTEND_URL=http://localhost:5173
